{
	"name": "frontend-ui",
	"version": "1.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build --configuration production --base-href /",
		"build-prod": "ng build --configuration production --base-href /",
		"build-uat": "ng build --configuration uat --base-href /",
		"build-qa": "ng build --configuration qa --base-href /",
		"build-dev": "ng build --configuration dev --base-href /",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"format:fix": "pretty-quick --staged",
		"format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
		"format:all": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run format:fix"
		}
	},
	"private": true,
	"dependencies": {
		"@angular-devkit/architect": "^0.1703.10",
		"@angular-devkit/core": "^17.3.10",
		"@angular/animations": "^17.3.12",
		"@angular/cdk": "^17.3.10",
		"@angular/common": "^17.3.12",
		"@angular/compiler": "^17.3.12",
		"@angular/core": "^17.3.12",
		"@angular/forms": "^17.3.12",
		"@angular/google-maps": "^16.2.14",
		"@angular/localize": "^17.3.12",
		"@angular/material": "^17.3.10",
		"@angular/material-moment-adapter": "^17.3.10",
		"@angular/platform-browser": "^17.3.12",
		"@angular/platform-browser-dynamic": "^17.3.12",
		"@angular/router": "^17.3.12",
		"@ckeditor/ckeditor5-angular": "^9.0.0",
		"@ckeditor/ckeditor5-build-classic": "^43.2.0",
		"@ng-bootstrap/ng-bootstrap": "^16.0.0",
		"@ng-select/ng-select": "^12.0.7",
		"@popperjs/core": "^2.10.2",
		"@types/google.maps": "^3.58.1",
		"angular-imask": "^6.4.2",
		"awesome-phonenumber": "^3.2.0",
		"bootstrap": "^5.3.3",
		"chart.js": "^3.8.0",
		"country-codes-list": "^1.6.8",
		"country-state-city": "^3.0.1",
		"file-saver": "^2.0.5",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"ng-circle-progress": "^1.7.1",
		"ng-gallery": "^11.0.0",
		"ng-otp-input": "^1.9.2",
		"ng2-charts": "^3.0.11",
		"ngx-infinite-scroll": "^17.0.0",
		"ngx-webcam": "^0.4.1",
		"rxjs": "~7.5.0",
		"signature_pad": "^4.2.0",
		"tslib": "^2.3.0",
		"xlsx": "^0.18.5",
		"zone.js": "~0.14.10"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^17.3.10",
		"@angular/cli": "^17.3.10",
		"@angular/compiler-cli": "^17.3.12",
		"@types/jasmine": "~3.10.0",
		"@types/lodash": "^4.14.182",
		"@types/node": "^12.11.1",
		"husky": "^8.0.1",
		"jasmine-core": "~4.0.0",
		"karma": "~6.3.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.1.0",
		"karma-jasmine": "~4.0.0",
